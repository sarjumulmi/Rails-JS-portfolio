<div class="container">
  <h2><%= @survey.title %></h2>
  <ul>
    <% @questions.each do |question| %>
      <% if question.question_text %>
        <li><%= question.question_text %></li>
      <% end %>
    <% end %>
  </ul>
  <%= link_to "Add Question", new_survey_question_path(@survey, @question) %>
  <%= link_to "Back To Survey", survey_path(@survey) %>
</div>
